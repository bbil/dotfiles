URxvt.font: xft:DejaVu Sans Mono:size=11
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Mod4-k: command:\033]720;1\007
URxvt.keysym.Mod4-j: command:\033]721;1\007
URxvt.iso14755: False

!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------

Xft.dpi:                    96
Xft.antialias:              false
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

!-------------------------------------------------------------------------------
! URxvt settings
! Colours lifted from Solarized (http://ethanschoonover.com/solarized)
! More info at:
! http://pod.tst.eu/http://cvs.schmorp.de/rxvt-unicode/doc/rxvt.1.pod
!-------------------------------------------------------------------------------

URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.fading:               0
! URxvt.urgentOnBell:         true
! URxvt.visualBell:           true
URxvt.loginShell:           true
URxvt.saveLines:            50
URxvt.internalBorder:       3
URxvt.lineSpace:            0

! Fonts
URxvt.allow_bold:           false
/URxvt URxvt.font:                 -URxvt-terminus-medium-r-normal-URxvt-12-120-72-72-c-60-iso8859-1 URxvt/
URxvtURxvtfont: xft:Monospace:pixelsize=14
URxvtURxvtboldFont: xft:Monospace:pixelsize=14

! Fix font space
URxvtURxvtletterSpace: -1

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false
URxvt.saveLines:            1000

! Perl extensions
URxvt.perl-ext-common:      default,matcher
URxvt.matcher.button:       1
URxvt.urlLauncher:          firefox

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #657b83
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true

! special
URxvt.foreground:   #a6a28c
URxvt.background:   #20201d
URxvt.cursorColor:  #a6a28c

! black
URxvt.color0:       #20201d
URxvt.color8:       #7d7a68

! red
URxvt.color1:       #d73737
URxvt.color9:       #d73737

! green
URxvt.color2:       #60ac39
URxvt.color10:      #60ac39

! yellow
URxvt.color3:       #cfb017
URxvt.color11:      #cfb017

! blue
URxvt.color4:       #6684e1
URxvt.color12:      #6684e1

! magenta
URxvt.color5:       #b854d4
URxvt.color13:      #b854d4

! cyan
URxvt.color6:       #1fad83
URxvt.color14:      #1fad83

! white
URxvt.color7:       #a6a28c
URxvt.color15:      #fefbec

